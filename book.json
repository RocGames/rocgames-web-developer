{
    "title": "Docs Protal",
    "description": "文档中心",
    "author": "Mob",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": "./docs",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",

        "sectionx",

        "ga",
        "code",
        "chapter-fold",
        "expandable-chapters",
        "expandable-chapters-small",
        "edit-link",
        "search-pro",
        "ancre-navigation",
        "splitter",
        "flexible-alerts",
        "auto-scroll-table",
        "popup",
        "theme-api",
        "page-footer-ex"
    ],
    "styles": {
        "website": "styles/website.css"
    },
    "pluginsConfig": {
        "chapter-fold":{},
        "expandable-chapters": {},
        "ga": {
            "token": "UA-44087823-2"
        },
        "theme-api": {
            "theme": "dark"
        },
        "edit-link": {
            "base": "https://github.com/RocGames/developer-docs-portal/edit/main/docs",
            "label": "编辑本页"
        },
        "sectionx": {
            "tag": "b"
        },
        "page-footer-ex":{
            "copyright": " Copyright © [ROC Games](http://roc.games) 2022 ",
            "markdown": true,
            "update_label": "此页面修订于：",
            "update_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}